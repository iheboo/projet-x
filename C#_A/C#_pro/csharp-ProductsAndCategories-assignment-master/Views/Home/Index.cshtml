@model List<Product>
@{
  ViewData["Title"] = "Home Page";
}
@{
  Product product = new Product();
}
<div class="container">
  <div class="row">
    <div class="col-md-6">@await Html.PartialAsync("_NewProduct", product)</div>
    <div class="col-md-6">@await Html.PartialAsync("_AllProducts")</div>
  </div>
</div>

